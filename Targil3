using System;

namespace homework3
{
    class Program
    {
        static void Main(string[] args)
        {
            int sum = 0, num;

            for (int i = 0; i < 20; i++)
            {
                num = int.Parse(Console.ReadLine());

                if (num > 2500) {
                    sum = sum + num;
                }
            }

            Console.WriteLine("The overall price of computers that cost more than 2500 is : " + sum);
        }
    }
}
